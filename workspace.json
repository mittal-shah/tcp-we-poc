{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "webclock",
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "projects": {
    "webclock": {
      "projectType": "application",
      "root": "apps/webclock",
      "sourceRoot": "apps/webclock/src",
      "prefix": "tcp",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/webclock",
            "index": "apps/webclock/src/index.html",
            "main": "apps/webclock/src/main.ts",
            "polyfills": "apps/webclock/src/polyfills.ts",
            "tsConfig": "apps/webclock/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/webclock/src/favicon.ico",
              "apps/webclock/src/assets",
              {
                "glob": "**/*",
                "input": "./libs/tcp-ng-ui/src/assets/",
                "output": "./assets"
              }
            ],
            "styles": [
              "apps/webclock/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/webclock/src/environments/environment.ts",
                  "with": "apps/webclock/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "webclock:build",
            "proxyConfig": "apps/webclock/src/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "webclock:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "webclock:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/webclock/src/**/*.ts",
              "apps/webclock/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/webclock"
          ],
          "options": {
            "jestConfig": "apps/webclock/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "webclock-e2e": {
      "root": "apps/webclock-e2e",
      "sourceRoot": "apps/webclock-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/webclock-e2e/cypress.json",
